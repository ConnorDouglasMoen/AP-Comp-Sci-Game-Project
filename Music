import java.applet.*;
import java.net.*;
import java.io.File;
public class Music {
	public void music() {
		     AudioClip clip;
			try {
				clip = Applet.newAudioClip(new File("Brodyquest.wav").toURL()); //Put the file into your project's folder.
				clip.loop();
			} catch (MalformedURLException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
}}
